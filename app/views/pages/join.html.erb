<div class="container">
  <div class="row">
    <div data-aos="zoom-in" class="col-xs-12 offset-md-3 col-md-6 white-container container-margin">
      <label class="text-center" for="url">Join the meeting</label>
      <input type="url" name="url" id="join-url" placeholder="Please paste the URL" required>
    </div>
  </div>
</div>

<script>
  const input = document.getElementById("join-url")

  input.addEventListener("keypress", (event) => {
    if(event.key === "Enter") {
      window.location = input.value
    }
  });
</script>
